{
  "name": "@arcblock/crawler-middleware",
  "version": "1.5.3",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.cjs.json --watch",
    "lint": "tsc --noEmit && eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "bundle": "npm run build",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "types": "./lib/cjs/index.d.ts"
    }
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@abtnode/cron": "^1.17.7",
    "@arcblock/crawler": "workspace:*",
    "@blocklet/logger": "^1.17.7",
    "@blocklet/sdk": "^1.17.7",
    "@sequelize/core": "7.0.0-alpha.46",
    "@sequelize/sqlite3": "7.0.0-alpha.46",
    "axios": "^1.7.9",
    "lru-cache": "^10.4.3",
    "queue": "^7.0.0",
    "sqlite3": "^5.1.7",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.19",
    "bumpp": "^9.11.1",
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.3",
    "zx": "^8.3.2"
  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
